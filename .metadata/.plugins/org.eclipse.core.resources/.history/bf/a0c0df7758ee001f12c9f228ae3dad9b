package service;

import java.util.List;
import dao.TarefaDao;  // Importando a classe TarefaDao
import model.Tarefa;

public class TarefaService {
    
    public List<Tarefa> listar() {
        TarefaDao dao = new TarefaDao();  // Usando a classe TarefaDao correta
        return dao.listarTarefas();  // Corrigindo o nome do m√©todo para 'listarTarefas'
    }
}
